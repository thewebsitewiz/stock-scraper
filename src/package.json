{
  "name": "stock scrape",
  "version": "1.0.0",
  "description": "",
  "main": "dist/scrape.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./process.ts",
    "start:watchrrr": "nodemon",
    "start-server": "./node_modules/.bin/ts-node ./process.ts",
    "start:watch": "./node_modules/.bin/nodemon  -w ./src --ext \".ts\" --exec \"npm run start-server\"",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "build": "rimraf ./build && tsc",
    "start:watch-dev": "./node_modules/.bin/nodemon  -w ./src --ext \".ts\" "
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "angular-src"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "argon2": "^0.28.5",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.22.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-jwt": "^7.6.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mongoose": "^6.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "password-validator": "^5.2.1",
    "path": "^0.12.7",
    "cheerio": "^1.0.0-rc.2",
    "chromium": "^3.0.3",
    "co": "^4.6.0",
    "ip": "^1.1.5",
    "knex": "^2.4.2",
    "mysql": "^2.16.0",
    "puppeteer": "^1.13.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "xmlhttprequest": "^1.8.0"
  }
}